import java.util.ArrayList;

public class Feed {
	protected ArrayList<String> privateMessages;
	protected ArrayList<String> publicMessages;
	
	public Feed() {
		
	}

	public Feed(ArrayList<String> privateMessages, ArrayList<String> publicMessages) {
		this.privateMessages = new ArrayList<>();
		this.publicMessages = new ArrayList<>();
	}

	public ArrayList<String> getPrivateMessages() {
		return privateMessages;
	}

	public void setPrivateMessages(ArrayList<String> privateMessages) {
		this.privateMessages = privateMessages;
	}

	public ArrayList<String> getPublicMessages() {
		return publicMessages;
	}

	public void setPublicMessages(ArrayList<String> publicMessages) {
		this.publicMessages = publicMessages;
	}

}
